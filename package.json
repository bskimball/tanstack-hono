{
	"name": "tanstack-hono",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"start": "cross-env NODE_ENV=production node dist/server/index.js",
		"build:client": "cross-env NODE_ENV=production vite build --mode client",
		"build:server": "cross-env NODE_ENV=production vite build",
		"build:types": "tsc",
		"build": "npm run build:client && npm run build:server && npm run build:types",
		"build:analyze": "npm run build:client -- --analyze",
		"test": "vitest run",
		"format": "prettier --write .",
		"lint": "eslint .",
		"check": "npm run lint && prettier --check ."
	},
	"dependencies": {
		"@hono/node-server": "^1.19.9",
		"@hono/zod-validator": "^0.7.6",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/react-devtools": "^0.9.2",
		"@tanstack/react-query": "^5.90.19",
		"@tanstack/react-router": "^1.153.2",
		"@tanstack/react-router-devtools": "^1.153.2",
		"@tanstack/router-plugin": "^1.153.2",
		"dotenv": "^17.2.3",
		"hono": "^4.11.4",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"tailwindcss": "^4.1.18",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@hono/vite-dev-server": "^0.24.1",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/react": "^16.3.2",
		"@types/node": "^25.0.9",
		"@types/react": "^19.2.9",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"babel-plugin-react-compiler": "^1.0.0",
		"cross-env": "^10.1.0",
		"eslint": "^9.39.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"globals": "^17.0.0",
		"jsdom": "^27.4.0",
		"prettier": "^3.8.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"vite": "^7.3.1",
		"vite-bundle-analyzer": "^1.3.2",
		"vitest": "^4.0.17",
		"web-vitals": "^5.1.0"
	}
}
